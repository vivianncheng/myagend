{
    "manifest_version": 3,
    "name": "MyAgend",
    "version": "1.1",
    "description": "A lightweight, private agenda extension for your everyday planning needs.",
    "action": {
        "default_popup": "main.html",
        "default_icon": {
            "48": "icons/icons8-calendar-50.png",
            "128": "icons/icons8-calendar-100.png"
        }
    },
    "permissions": [
        "storage"
    ]
}